feature titl {
 # Titling
 script latn; # Latin
    sub [A O V o] by [A.2 o.2 V.2 o.2];
 language TRK ; # Turkish
 language ROM ; # Romanian
} titl;

feature ss02 {
 # Stylistic Set 2
 script latn; # Latin
    sub [A O V o] by [A.2 o.2 V.2 o.2];
 language TRK ; # Turkish
 language ROM ; # Romanian
} ss02;

feature ss03 {
 # Stylistic Set 3
 # DEFAULT
lookup ss035 {
    sub [A O V o] by [A.2 o.2 V.2 o.2];
} ss035;
lookup ss036 {
    sub o.2 o.2' by o;
    sub o.2 @ss031 o.2' by o;
    sub o.2 @ss031 @ss031 o.2' by o;
    sub o.2 @ss031 @ss031 @ss031 o.2' by o;
    sub A.2 A.2' by A;
    sub A.2 @ss031 A.2' by A;
    sub A.2 @ss031 @ss031 A.2' by A;
    sub A.2 @ss031 @ss031 @ss031 A.2' by A;
    sub V.2 V.2' by V;
    sub V.2 @ss031 V.2' by V;
    sub V.2 @ss031 @ss031 V.2' by V;
    sub V.2 @ss031 @ss031 @ss031 V.2' by V;
} ss036;
 script latn; # Latin
lookup ss035;
lookup ss036;
 language TRK ; # Turkish
 language ROM ; # Romanian
} ss03;

feature ss04 {
  sub delta by delta.ss04;
  sub zeta by zeta.ss04;
  sub xi by xi.ss04;
} ss04;

