feature liga {
 # Standard Ligatures
 # DEFAULT
lookup liga1 {
    sub F A T A M by Fam_ligne;
    sub F A T A m by Fam_ligne;
    sub F A T a m by Fam_ligne;
    sub F A t a m by Fam_ligne;
    sub F a t a m by Fam_ligne;
    sub f a t a m by Fam_ligne;
} liga1;
lookup liga2 {
    sub f r a' n ccedilla o i s by aacute;
    sub F r a' n ccedilla o i s by aacute;
    sub F R A' N Ccedilla O I S by Aacute;
} liga2;
lookup liga3 {
    sub W W W by WWW;
    sub w w w by WWW;
} liga3;
 script latn; # Latin
lookup liga1;
lookup liga2;
lookup liga3;
 language TRK ; # Turkish
 language ROM ; # Romanian
} liga;

